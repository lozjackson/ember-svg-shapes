<h2>svg-star</h2>

<pre><code>&#123;&#123;svg-star
  size="number"
  strokeWidth="number"
  rotate="number of degrees"
  starPoints="number"
  innerPoints="number"
  filter="filter id"
  class="class names"&#125;&#125;</code></pre>

<div>
    <div>
        size: {{size}}
    </div>
    <div>
        {{input type="range" value=size min="1" max="500" step="1" class="range-slider"}}
    </div>
</div>
<div>
    <div>
        strokeWidth: {{strokeWidth}}
    </div>
    <div>
        {{input type="range" value=strokeWidth min="0" max="100" step="1" class="range-slider"}}
    </div>
</div>
<div>
    <div>
        rotate: {{rotateStar}}
    </div>
    <div>
        {{input type="range" value=rotateStar min="-360" max="360" step="0.1" class="range-slider"}}
    </div>
</div>
<div>
    <div>
        starPoints: {{starPoints}}
    </div>
    <div>
        {{input type="range" value=starPoints min="3" max="100" step="1" class="range-slider"}}
    </div>
</div>
<div>
    <div>
        innerPoints: {{starInnerPoints}}
    </div>
    <div>
        {{input type="range" value=starInnerPoints min="0.01" max="1.99" step="0.01" class="range-slider"}}
    </div>
</div>
<div align="center">
    {{svg-star size=size rotate=rotateStar fill="blue" strokeWidth=strokeWidth starPoints=starPoints innerPoints=starInnerPoints }}
</div>
