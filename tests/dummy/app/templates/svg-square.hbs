<h2>svg-square</h2>

<pre><code>&#123;&#123;svg-square
  size="number"
  strokeWidth="number"
  rotate="number of degrees"
  radiusX="number"
  radiusY="number"
  fill="color"
  filter="filter id"
  class="class names"&#125;&#125;</code></pre>

<div>
    <div>
        size: {{size}}
    </div>
    <div>
        {{input type="range" value=size min="1" max="500" step="1" class="range-slider"}}
    </div>
</div>
<div>
    <div>
        strokeWidth: {{strokeWidth}}
    </div>
    <div>
        {{input type="range" value=strokeWidth min="0" max="100" step="1" class="range-slider"}}
    </div>
</div>
<div>
    <div>
        rotate: {{rotate}}
    </div>
    <div>
        {{input type="range" value=rotate min="-360" max="360" step="0.1" class="range-slider"}}
    </div>
</div>
<div>
    <div>
        radiusX: {{radiusX}}
    </div>
    <div>
        {{input type="range" value=radiusX min="0" max="100" step="0.1" class="range-slider"}}
    </div>
</div>
<div>
    <div>
        radiusY: {{radiusY}}
    </div>
    <div>
        {{input type="range" value=radiusY min="0" max="100" step="0.1" class="range-slider"}}
    </div>
</div>

<div align="center">
    {{svg-square size=size rotate=rotate fill="blue" strokeWidth=strokeWidth radiusX=radiusX radiusY=radiusY}}
</div>
